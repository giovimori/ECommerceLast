@model dbContext
@{
    ViewData["Title"] = "Registrato";
}

<h2>Grazie @Model.Nome</h2>
 <p>per esserti registrato</p>
<p>Sono stati registrati @Model.Utenti.Count() utenti... </p>

<table class="table">
    <thead>
        <th>Nome</th>
        <th>Cognome</th>
        <th>Email</th>
        <th>Password</th>
        <th>Indirizzo</th>
    </thead>
    @{ 
        foreach(var item in @Model.Utenti) {
            <tr>
                <td>@item.Nome</td>
                <td>@item.Cognome</td>
                <td>@item.EMail</td>
                <td>@item.Password</td>
                <td>@item.Indirizzo</td>
            </tr>
        }
    }
         <h4><a asp-area="" asp-controller="Home" asp-action="Farmaci">VAI ALLO SHOP</a></h4>

</table>